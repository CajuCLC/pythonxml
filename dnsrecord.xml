               <TTL>3600</TTL>
               <ResourceRecords>
                  <ResourceRecord>
                     <Value>web-04.signashop.com.br.</Value>
                  </ResourceRecord>
               </ResourceRecords>
            </ResourceRecordSet>
         </Change>
      </Changes>
   </ChangeBatch>
</ChangeResourceRecordSetsRequest>
